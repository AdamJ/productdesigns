{
  "name": "productdesigns",
  "version": "1.0.0",
  "description": "The website for productdesigns.net - where all of my product designs are showcased",
  "keywords": [
    "product",
    "designs",
    "product",
    "designs"
  ],
  "homepage": "https://github.com/AdamJ/productdesigns#readme",
  "bugs": {
    "url": "https://github.com/AdamJ/productdesigns/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdamJ/productdesigns.git"
  },
  "license": "ISC",
  "author": "Adam J. Jolicoeur",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "sass:build": "mkdir -p _site/assets/css && sass src/assets/scss/site.scss _site/assets/css/site.css --style=compressed",
    "sass:watch": "mkdir -p _site/assets/css && sass --watch src/assets/scss:_site/assets/css --style=expanded",
    "eleventy:build": "eleventy",
    "eleventy:serve": "eleventy --serve",
    "prebuild": "npm run sass:build",
    "build": "npm run eleventy:build",
    "prestart": "npm run sass:build",
    "start": "npm-run-all --parallel sass:watch eleventy:serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.94.2",
    "vite": "^5.0.0"
  }
}
